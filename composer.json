{
    "name": "symplify/psr4-switcher",
    "description": "Switch classmap to PSR-4",
    "license": "MIT",
    "bin": [
        "bin/psr4-switcher"
    ],
    "require": {
        "php": ">=7.3",
        "nette/utils": "^3.0",
        "nette/robot-loader": "^3.2",
        "symfony/dependency-injection": "^5.1",
        "symfony/console": "^4.4|^5.1",
        "symfony/http-kernel": "^4.4|^5.1",
        "symplify/composer-json-manipulator": "^9.1",
        "symplify/symplify-kernel": "^9.1",
        "symplify/package-builder": "^9.1",
        "symplify/smart-file-system": "^9.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\Psr4Switcher\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\Psr4Switcher\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
